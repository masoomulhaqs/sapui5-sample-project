{
	"_version": "1.8.0",
	"sap.app": {
		"id": "sapui5-sample-project.sapui5-sample-project",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"sourceTemplate": {
			"id": "ui5template.basicSAPUI5ApplicationProject",
			"version": "1.40.12"
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": ["sap_hcb", "sap_belize"]
	},
	"sap.ui5": {
		"rootView": {
			"viewName": "sapui5-sample-project.sapui5-sample-project.view.App",
			"type": "XML",
			"async": true
		},
		"dependencies": {
			"minUI5Version": "1.60.1",
			"libs": {
				"sap.ui.layout": {},
				"sap.ui.core": {},
				"sap.m": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "sapui5-sample-project.sapui5-sample-project.i18n.i18n"
				}
			}
		},
		"resources": {
			"css": [{
				"uri": "css/style.css"
			}]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"async": true,
				"viewPath": "sapui5-sample-project.sapui5-sample-project.view",
				"controlAggregation": "pages",
				"controlId": "rootControl",
				"clearControlAggregation": false,
				"viewLevel": 1,
				"bypassed": {
					"target": []
				}
			},
			"routes": [{
				"name": "page1",
				"pattern": "",
				"target": ["page1"]
			}, {
				"name": "page2",
				"pattern": "",
				"target": ["page2"]
			}],
			"targets": {
				"page1": {
					"viewType": "XML",
					"transition": "slide",
					"clearControlAggregation": false,
					"viewName": "View1",
					"viewLevel": 0
				},
				"page2": {
					"viewType": "XML",
					"transition": "slide",
					"clearControlAggregation": false,
					"viewName": "View2",
					"viewLevel": 1
				}
			}
		}
	}
}